<?xml Msvcver="1.0" encoding="utf-8"?> 
<Project ToolsMsvcver="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <MPIR_Conftype Condition="'$(ConfigurationType)' == 'StaticLibrary'">lib</MPIR_Conftype>
    <MPIR_Conftype Condition="'$(ConfigurationType)' == 'DynamicLibrary'">dll</MPIR_Conftype>
  </PropertyGroup>

  <PropertyGroup>
    <MPIR_ProjectPropsName>$(ProjectName)</MPIR_ProjectPropsName>

    <MPIR_ProjectProps     >$(MPIR_ProjectPropsName).props</MPIR_ProjectProps>
  	<MPIR_ProjectConfProps >$(MPIR_ProjectPropsName).$(Configuration.ToLower()).props</MPIR_ProjectConfProps>
  	<MPIR_ProjectPlatfProps>$(MPIR_ProjectPropsName).$(Platform.ToLower()).props</MPIR_ProjectPlatfProps>
  </PropertyGroup>

  <PropertyGroup>
    <MPIR_MsvcverPropsName>$(PlatformToolsetVersion)<MPIR_MsvcverPropsName>
    <MPIR_MsvcverPropsDir>..\_props\<MPIR_MsvcverPropsDir>

    <MPIR_MsvcverProps        >$(MPIR_MsvcverPropsDir)$(MPIR_MsvcverPropsName).props</MPIR_LocalProps>
    <MPIR_MsvcverConftypeProps>$(MPIR_MsvcverPropsDir)$(MPIR_MsvcverPropsName).$(MPIR_Conftype).props</MPIR_MsvcverConftypeProps>
    <MPIR_MsvcverConfProps    >$(MPIR_MsvcverPropsDir)$(MPIR_MsvcverPropsName).$(Configuration.ToLower()).props</MPIR_LocalConfProps>
    <MPIR_MsvcverPlatfProps   >$(MPIR_MsvcverPropsDir)$(MPIR_MsvcverPropsName).$(Platform.ToLower()).props</MPIR_LocalPlatfProps>
  </PropertyGroup>

  <PropertyGroup>
    <MPIR_CommonPropsName>Common<MPIR_CommonPropsName>
    <MPIR_CommonPropsDir>..\..\build.vc\<MPIR_CommonPropsDir>

    <MPIR_CommonProps        >$(MPIR_CommonPropsDir)$(MPIR_CommonPropsName).props</MPIR_LocalProps>
    <MPIR_CommonConftypeProps>$(MPIR_CommonPropsDir)$(MPIR_CommonPropsName).$(MPIR_Conftype).props</MPIR_CommonConftypeProps>
    <MPIR_CommonConfProps    >$(MPIR_CommonPropsDir)$(MPIR_CommonPropsName).$(Configuration.ToLower()).props</MPIR_LocalConfProps>
    <MPIR_CommonPlatfProps   >$(MPIR_CommonPropsDir)$(MPIR_CommonPropsName).$(Platform.ToLower()).props</MPIR_LocalPlatfProps>
  </PropertyGroup>

</Project>
